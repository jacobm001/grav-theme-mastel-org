{% extends 'partials/base.html.twig' %}

{% block content %}
{% for module in page.collection() %}
    <div id="{{ _self.pageLinkName(module.menu) }}"></div>
    <div class="row mb-2">
        {{ module.content }}
    </div>

    {% if not loop.last %}
    <hr>
    {% endif %}
{% endfor %}
{% endblock %}
